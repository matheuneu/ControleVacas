<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="c.controle.controlevacas">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/vaca"
        android:label="@string/app_name"
        android:roundIcon="@drawable/vaca"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CCSLista"></activity>
        <activity android:name=".CadastrarCcs"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".CadastrarCmt"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".CmtLista" />
        <activity
            android:name=".CadastrarControleLeiteiro"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".ControleLeiteiroLista" />
        <activity
            android:name=".CadastrarProdutor"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".ProdutoresLista" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="banco_controlevacas.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="c.controle.controlevacas" />
    </application>

</manifest>